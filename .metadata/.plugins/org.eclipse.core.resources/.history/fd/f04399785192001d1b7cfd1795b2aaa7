package 메서드;

public class Ex09메서드예제 {
	public static void main(String[] args) {
		
		int num1 = 10;
		int num2 = 2;
		boolean divisor = isDivisor(num1,num2);
		
		System.out.println(divisor);
	}
	
	public static boolean isDivisor(int num1, int num2) {
		return (num1%num2==0) ? true : false ;
	}

}
