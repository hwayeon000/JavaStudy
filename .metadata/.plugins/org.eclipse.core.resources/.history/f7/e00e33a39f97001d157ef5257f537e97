public class TestClass {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		Random rd = new Random();
		
//		int[][] arr = new int[5][5];
		int cnt = 10;
		int[] coun = {6, 2, 5, 5, 4, 5, 6, 3, 7, 6};
		
		System.out.print("입력 >> ");
		int a = sc.nextInt();
//		char[] arr = new char[coun.length];
		int res = 0;
		while (a>0) {
			res += coun[a%10];
			a /= 10;
		}

		System.out.println(res);
		
		
		
		
	}
	

	
