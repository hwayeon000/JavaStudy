package 메서드;

public class Ex08메서드예제3 {
	public static void main(String[] args) {
		
		int num1 = 50;
		int num2 = 15;
		String op = "-";
		
		System.out.println(cal(num1, num2, op));
		
		
	}

	public static int cal(int num1, int num2, String op) {
		
		int res = (op.equals("*")) ? num1 * num2 : num1 - num2;
		
		return res;
	}
}
