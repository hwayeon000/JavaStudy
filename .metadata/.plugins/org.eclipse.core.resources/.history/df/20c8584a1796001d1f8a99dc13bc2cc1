package 상품관리프로그램;

public class Product {

	private String name;
	// 단가
	private int unitPrice;
	// 수량
	private int amount;

	public Product(String name, int unitPrice, int amount) {
		super();
		this.name = name;
		this.unitPrice = unitPrice;
		this.amount = amount;
	}

	public String getName() {
		return name;
	}

	public int getUnitPrice() {
		return unitPrice;
	}

	public int getAmount() {
		return amount;
	}

}
