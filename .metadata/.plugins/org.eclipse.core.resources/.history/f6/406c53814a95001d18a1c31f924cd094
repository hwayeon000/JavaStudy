package 클래스구조;

public class AddressMain {
	public static void main(String[] args) {
		// 클래스명 객체명 = new 클래스명() ;
		Address addr;
		addr = new Address();
		
		// 객체의 필드, 메소드 사용 방법은 .을 이용한다.
		
		addr.name = "김미희";
		addr.age = 20;
		addr.phoneNumber = "010-2407-8429";
		
		addr.showAddress();
		
		
	}

}
